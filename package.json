{
  "name": "browser-plus",
  "displayName": "browser-plus",
  "description": "Browser in VSCode",
  "version": "0.0.1",
  "publisher": "bobjoy",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.openBrowser"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.openBrowser",
        "title": "Open Browser"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
  "dependencies": {
    "browser-sync": "^2.18.8",
    "detect-port": "^1.1.0",
    "mz": "^2.6.0",
    "portfinder": "^1.0.13"
  }
}
